    "bcfg": "^0.1.6",
    "browser-or-node": "^1.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.26",
    "express": "^4.17.1",
    "level": "^6.0.1",
    "levelup": "^4.4.0",
    "node-fetch": "^2.6.1"
