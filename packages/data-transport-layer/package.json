    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "hardhat": "^2.0.9",
    "mocha": "^8.3.2",
    "pino-pretty": "^4.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
